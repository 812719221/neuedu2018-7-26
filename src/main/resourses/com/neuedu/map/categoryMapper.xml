<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.neuedu.entity.Category">
    <insert id="addCategory" parameterType="com.neuedu.entity.Category">
        INSERT INTO category (parent_id,name,status,sort_order,create_time,update_time)  values
        (#{parent_id},#{name},#{status},#{sort_order},#{create_time},#{update_time})
    </insert>
</mapper>